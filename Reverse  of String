class Reverse
{
    // Complete the function
    // str: input string
    public static String reverseWord(String str)
    {  
        String str1="";
        int len=str.length();
        for(int i=len-1;i>=0;i--)
        {
           //System.out.println(str.charAt(i)) ;
           str1+=str.charAt(i);
        }
        return str1;
    }
}
